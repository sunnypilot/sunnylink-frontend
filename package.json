		"openapi-fetch": "^0.13.5",
		"openapi-typescript": "^7.6.1",
		"svelte-sonner": "^0.3.28",
		"theme-change": "^2.5.0",
		"vite-plugin-node-polyfills": "^0.23.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@flydotio/dockerfile": "^0.7.10",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^4.0.0",
